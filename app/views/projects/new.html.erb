<h2> Add Project: </h2>
<%= form_for @project, url: {action: "create"} do |f| %>
  <%= label_tag "Customer Company" %>
  <%= select_tag :customer, options_for_select(Customer.pluck(:company))%>
  <%=f.label 'Project Name:' %>
  <%=f.text_field :project_name %>
  <%=f.submit "Add Project", :class => "btn btn-primary" %>
<% end %>